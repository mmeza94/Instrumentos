@using MyResource = Laboratorio.Messages;
<!DOCTYPE html>
<html>
<head>
    <title>@MyResource.Instruments</title>
    <link href="@Url.Content("~/Content/Styles/TenarisWebApplications.css")" rel="stylesheet" type="text/css" />
    @*<link href="@Url.Content("~/Content/themes/base/jquery.ui.theme.css")" rel="stylesheet" type="text/css" />*@
    @*<link href="@Url.Content("~/Content/themes/base/jquery.ui.datepicker.css")" rel="stylesheet" type="text/css" />*@
    <link href="@Url.Content("~/Content/Styles/customStyle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Styles/SweetAlertStyle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Styles/DataTable.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Styles/jquery-ui.min.css")" rel="stylesheet" type="text/css" />



    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-3.5.1.js")" type="text/javascript"></script>
    @*<script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>*@
    @*<script src="@Url.Content("~/Scripts/datePicker-ui-ES.js")" type="text/javascript"></script>*@
    <script src="@Url.Content("~/Scripts/SweetAlert.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/DataTable.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui.min.js")" type="text/javascript"></script>
    



    

</head>
<body style="background-color:white;overflow:auto;padding-bottom:50px">
    <div style="height: 64px; background-color: White; width: 100%">
        <table style="width: 100%" cellpadding="0" cellspacing="0">
            <colgroup>
                <col style="width: 25%" />
                <col style="width: 50%" />
                <col style="width: 25%" />
            </colgroup>
            <tr style="height: 53px">
                <td>
                    <div style="text-align: left; padding-left: 8px">
                        <img src="@Url.Content("~/Content/images/TenarisLogo.png")" alt="@MyResource.TenarisLogo" /></div>
                </td>
                <td class="headerTextStyle">
                    @MyResource.Instruments
                </td>
                <td class="logout">
                    @if (Request.IsAuthenticated)
                    {
                        @Html.ActionLink("Salir", "LogOut", "Account")
                    }
                </td>
            </tr>
            <tr style="height: 2px">
                <td style="background-color: #00009C">
                </td>
                <td style="background-color: #CE0063">
                </td>
                <td style="background-color: #009A00">
                </td>
            </tr>
            <tr style="height: 5px">
                <td style="background-color: White">
                </td>
                <td style="background-color: White">
                </td>
                <td style="background-color: White">
                </td>
            </tr>
        </table>
    </div>
    @if (Request.IsAuthenticated)
    {
        <div class="footer colorbase">
            <p style="font-size:12pt">
                @Html.ActionLink(MyResource.ToolType, "Index", "ToolTypes")
                |
                @Html.ActionLink(MyResource.Instruments, "Index", "Tool")
                |
                @Html.ActionLink(MyResource.Assignment, "Index", "Assignment")
                |
                @Html.ActionLink("Instrumentos deshabilitados", "Index", "DischargedTools")
                |
                @Html.ActionLink("Kits de instrumentos", "Index", "Toolkit")
            </p>
        </div>
    }
    @RenderBody()
</body>
</html>
